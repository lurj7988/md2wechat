{
  "name": "md2wechat",
  "version": "2.0.0",
  "description": "Markdown to WeChat Official Account converter",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "md2wechat": "./bin/md2wechat"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "files": [
    "dist",
    "themes",
    "bin",
    "assets"
  ],
  "scripts": {
    "build": "tsc && npx tsc-alias -f",
    "watch": "tsc --watch",
    "dev": "node --loader ts-node/esm src/cli/index.ts",
    "convert": "node dist/cli/index.js convert",
    "sync-md": "node dist/cli/index.js sync-md",
    "sync-html": "node dist/cli/index.js sync-html",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "markdown",
    "wechat",
    "weixin",
    "official-account",
    "cli",
    "converter",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "highlight.js": "^11.9.0",
    "juice": "^10.0.0",
    "katex": "^0.16.9",
    "markdown-it": "^14.0.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
